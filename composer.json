{
  "name": "makg/user-bundle",
  "description": "Simple User Bundle with useful features. Only Doctrine ORM is supported.",
  "type": "symfony-bundle",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "MakG",
      "email": "makg@makg.eu"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/dependency-injection": "^4.2",
    "symfony/http-kernel": "^4.2",
    "symfony/config": "^4.2",
    "symfony/orm-pack": "^1.0",
    "symfony/security": "^4.2",
    "symfony/form": "^4.2",
    "symfony/templating": "^4.2",
    "symfony/twig-bundle": "^4.2",
    "symfony/translation": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "MakG\\UserBundle\\": ""
    }
  },
  "require-dev": {
    "easycorp/easyadmin-bundle": "^2.0",
    "phpunit/phpunit": "^8.0",
    "yzalis/identicon": "^1.2",
    "swiftmailer/swiftmailer": "^6.1.3",
    "symfony/mailer": "^4.4"
  },
  "suggest": {
    "yzalis/identicon": "^1.2"
  }
}
